		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/node": "^20.8.0",
		"@types/vscode": "^1.82.0",
		"@vscode/vsce": "^2.21.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.4",
		"@vscode/vsce": "^2.21.1",
		"tslint": "^5.12.1",
		"@types/mocha": "^2.2.42"
	}
